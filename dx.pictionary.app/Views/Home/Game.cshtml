@model dx.pictionary.app.Controllers.GameViewModel

@{
    ViewBag.Title = @Model.GameId;
}
<div class="row">
    <div class="col-lg-12">
        <h2>@Model.GameId</h2>
    </div>
</div>

<div class="row">
    <div class="col-lg-12">

        <div class="panel panel-success">
            <div class="panel-heading">
                <h3 class="panel-title">@Model.ProductFeature.Category</h3>
            </div>
            <div class="panel-body">
                @Model.ProductFeature.Name
            </div>
        </div>
    </div>
</div>

<div class="row">
    <div class="col-lg-12">
        <div class="btn-group btn-group-justified">
            <a href="#" id="clockStart" class="btn btn-default">Start</a>
            <a href="#" id="clockStop" class="btn btn-default">Stop</a>
            <a href="#" id="clockReset" class="btn btn-default">Reset</a>
        </div>
    </div>
</div>
<div class="row form-horizontal">
    &nbsp;
</div>
<div class="row form-horizontal">
        <div class="col-lg-12">
            <div class="your-clock"></div>
        </div>
    </div>

