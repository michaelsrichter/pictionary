@model dx.pictionary.app.Controllers.GameViewModel

@{
    ViewBag.Title = @Model.GameId;
}
<div class="row">
    <div class="col-lg-12">
        <h2>@Model.GameId</h2>
    </div>
</div>

<div class="row">
    <div class="col-lg-12">

        <div class="panel panel-success">
            <div class="panel-heading">
                <h3 class="panel-title">@Model.ProductFeature.Category</h3>
            </div>
            <div class="panel-body">
                @Model.ProductFeature.Name
            </div>
        </div>
    </div>
</div>